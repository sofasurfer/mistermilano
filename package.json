{
  "name": "wptheme.starter",
  "private": true,
  "version": "1.0.0",
  "description": "⚡️ Neofluxe starter theme",
  "main": "index.js",
  "repository": "git@github.com:cubegrafik/wptheme.starter.git",
  "author": "erin@neofluxe.ch",
  "license": "MIT",
  "engines": {
    "node": ">=18.4.0"
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19",
    "npm": "8.12.1"
  },
  "scripts": {
    "dev": "bud dev",
    "build": "bud build",
    "clean": "bud clean",
    "translate": "npm run translate:pot && npm run translate:js",
    "translate:pot": "wp i18n make-pot . ./resources/lang/sage.pot --ignore-domain --include=\"app,resources\"",
    "translate:js": "wp i18n make-json ./resources/lang --pretty-print",
    "test": "mocha"
  },
  "devDependencies": {
    "@roots/bud": "^6.6.6",
    "@roots/bud-postcss": "^6.6.6",
    "@roots/bud-preset-wordpress": "^6.6.5",
    "@roots/bud-sass": "^6.6.6",
    "mocha": "^10.0.0"
  },
  "dependencies": {
    "@wordpress/block-editor": "^9.6.0",
    "@wordpress/blocks": "^11.13.0",
    "@wordpress/components": "^19.16.0",
    "@wordpress/i18n": "^4.14.0"
  }
}
